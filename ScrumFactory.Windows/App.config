<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="ScrumFactory.Windows.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <appSettings>
    <add key="ProjectsServiceUrl" value="$ScrumFactoryServer$/ProjectsService/" />
    <add key="BacklogServiceUrl" value="$ScrumFactoryServer$/BacklogService/" />
    <add key="TeamServiceUrl" value="$ScrumFactoryServer$/TeamService/" />
    <add key="TasksServiceUrl" value="$ScrumFactoryServer$/TasksService/" />
    <add key="ProposalsServiceUrl" value="$ScrumFactoryServer$/ProposalsService/" />
    <add key="ArtifactsServiceUrl" value="$ScrumFactoryServer$/ArtifactsService/" />
